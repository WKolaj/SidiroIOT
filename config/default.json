{
  "tokenHeader": "x-auth-token",
  "port": 5000,
  "settingsPath": "settings",
  "userFileName": "userSettings.json",
  "netplanConfigSocketFilePath": "settings/netplanConfigSocket.socket",
  "logging": {
    "logActions": true,
    "info": {
      "path": "logs/info.log",
      "maxsize": 10485760,
      "maxFiles": 10
    },
    "warning": {
      "path": "logs/warn.log",
      "maxsize": 10485760,
      "maxFiles": 10
    },
    "error": {
      "path": "logs/error.log",
      "maxsize": 10485760,
      "maxFiles": 10
    }
  }
}
